# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Twitter API (twitterapi.io)
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_BASE_URL=https://api.twitterapi.io/v1
TWITTER_API_AUTH_STYLE=x-api-key # opciones: x-api-key | bearer | apikey
# Opcional: override de rutas seg√∫n docs
# Ejemplos comunes: /twitter/user/profile  o  /users/by/username
TWITTER_API_PROFILE_PATH=
# Ejemplos comunes: /twitter/user/tweets  o  /user/tweets
TWITTER_API_TWEETS_PATH=

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-1.5-pro
